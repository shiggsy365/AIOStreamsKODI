<include content="DefaultBackground" />
	<control type="group">
		<visible>!Player.HasMedia</visible>
		<!-- Background Fanart -->
		<control type="image">
			<depth>DepthBackground</depth>
			<include>FullScreenDimensions</include>
			<texture background="true" colordiffuse="BBFFFFFF">$VAR[CurrentWidgetFanart]</texture>
			<fadetime>600</fadetime>
			<aspectratio>scale</aspectratio>
		</control>
		
		<!-- Info Overlay -->
		<control type="group">
			<left>150</left>
			<top>50</top>
			<width>1100</width>
			<height>500</height>
			<animation effect="fade" start="0" end="100" time="400">Visible</animation>
			<animation effect="fade" start="100" end="0" time="400">Hidden</animation>
			<visible>!String.IsEmpty(Container($VAR[CurrentWidgetListID]).ListItem.Label)</visible>

			<!-- ClearLogo -->
			<control type="image">
				<left>0</left>
				<top>0</top>
				<width>500</width>
				<height>180</height>
				<aspectratio align="left" aligny="bottom">keep</aspectratio>
				<texture background="true">$VAR[CurrentWidgetClearLogo]</texture>
			</control>

			<!-- Info Title -->
			<control type="label">
				<left>0</left>
				<top>200</top>
				<width>1000</width>
				<height>40</height>
				<font>font30_title</font>
				<textcolor>FFFFFFFF</textcolor>
				<label>$VAR[CurrentWidgetInfo]</label>
				<shadowcolor>text_shadow</shadowcolor>
			</control>

			<!-- Plot -->
			<control type="textbox">
				<left>0</left>
				<top>250</top>
				<width>1200</width>
				<height>200</height>
				<font>font15</font>
				<textcolor>BBBBBBBB</textcolor>
				<label>$VAR[CurrentWidgetPlot]</label>
				<shadowcolor>text_shadow</shadowcolor>
				<autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
			</control>
		</control>
	</control>

