<?xml version="1.0"encoding="UTF-8"?>
<includes>
    <!--Default Button -->
    <default type="button">
        <height>60</height>
        <width>200</width>
        <font>font27</font>
        <textcolor>FFFFFFFF</textcolor>
        <focusedcolor>ThemeColor</focusedcolor>
        <disabledcolor>FF666666</disabledcolor>
        <align>center</align>
        <aligny>center</aligny>
    </default>

    <!--Default Label -->
    <default type="label">
        <height>40</height>
        <font>font27</font>
        <textcolor>FFFFFFFF</textcolor>
        <align>left</align>
        <aligny>center</aligny>
    </default>

    <!--Default List -->
    <default type="list">
        <height>500</height>
        <width>800</width>
        <orientation>vertical</orientation>
        <pagecontrol></pagecontrol>
        <scrolltime>200</scrolltime>
    </default>

    <!--Default Image -->
    <default type="image">
        <aspectratio>keep</aspectratio>
    </default>

    <!--Default Edit -->
    <default type="edit">
        <height>60</height>
        <font>font27</font>
        <textcolor>FFFFFFFF</textcolor>
    </default>

    <!--Default RadioButton -->
    <default type="radiobutton">
        <height>60</height>
        <width>300</width>
        <font>font27</font>
        <textcolor>FFFFFFFF</textcolor>
        <focusedcolor>ThemeColor</focusedcolor>
    </default>

    <!--Default Slider -->
    <default type="slider">
        <height>60</height>
        <width>400</width>
    </default>
</includes>
